{
    "name": "cartalyst/dependencies",
    "description": "Simple dependency resolution and sorting.",
    "keywords": [
        "platform",
        "ecommerce",
        "laravel",
        "bootstrap"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Cartalyst LLC",
            "email": "help@cartalyst.com",
            "homepage": "https://cartalyst.com"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "cartalyst/php-cs-fixer-config": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Cartalyst\\Dependencies\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cartalyst\\Dependencies\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "component": "package",
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
